# Configurações universais para qualquer plataforma
legacy-peer-deps=true
strict-ssl=false
registry=https://registry.npmjs.org/
save-exact=false

# Ignorar pacotes opcionais (resolve Rollup binários por SO)
omit=optional

# Configurações adicionais para resolver problemas de dependências
fund=false
audit=false

# Forçar uso de módulos JavaScript em vez de binários nativos
rollup__prefer_js=true